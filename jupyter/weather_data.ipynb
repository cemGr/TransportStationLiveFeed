    "from tqdm import tqdm  # Optional für Fortschrittsanzeige\n",
    "\n",
    "all_weather_frames = []\n",
    "\n",
    "# Nur die ersten 50 Trips\n",
    "for idx, row in tqdm(df.head(50).iterrows(), total=50):\n",
    "    try:\n",
    "        weather = fetch_hourly_weather(row['start_lat'], row['start_lon'], row['time_rounded'])\n",
    "        weather['trip_index'] = idx\n",
    "        all_weather_frames.append(weather)\n",
    "    except Exception as e:\n",
    "        print(f\"Fehler bei Trip {idx}: {e}\")\n",
    "\n",
    "# Zusammenführen\n",
    "weather_df = pd.concat(all_weather_frames, ignore_index=True)\n",
    "\n",
    "# Zurück mergen, falls gewünscht:\n",
    "df_small = df.head(50).reset_index().rename(columns={'index': 'trip_index'})\n",
    "df_with_weather = df_small.merge(weather_df, on='trip_index', how='left')\n"
   ]
  },
